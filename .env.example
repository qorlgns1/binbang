# 카카오 API 설정
# https://developers.kakao.com 에서 앱 생성 후 발급
KAKAO_REST_API_KEY=your_rest_api_key_here
KAKAO_CLIENT_SECRET=your_client_secret_here
KAKAO_REDIRECT_URI=http://localhost:3000/callback

# 스케줄 설정 (cron 표현식)
# 기본: 10분마다
SCHEDULE=*/10 * * * *

# 체크 간격 (밀리초) - 여러 숙소 체크 시 사이 간격
CHECK_INTERVAL=5000
