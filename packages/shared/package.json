{
  "name": "@workspace/shared",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "development": "./src/types/index.ts",
      "default": "./dist/types/index.js"
    },
    "./checkers": {
      "types": "./src/checkers/index.ts",
      "development": "./src/checkers/index.ts",
      "default": "./dist/checkers/index.js"
    },
    "./url-parser": {
      "types": "./src/urlParser.ts",
      "development": "./src/urlParser.ts",
      "default": "./dist/urlParser.js"
    },
    "./urlParser": {
      "types": "./src/urlParser.ts",
      "development": "./src/urlParser.ts",
      "default": "./dist/urlParser.js"
    },
    "./i18n": {
      "types": "./src/i18n/index.ts",
      "development": "./src/i18n/index.ts",
      "default": "./dist/i18n/index.js"
    },
    "./utils/date": {
      "types": "./src/utils/date.ts",
      "development": "./src/utils/date.ts",
      "default": "./dist/utils/date.js"
    },
    "./utils/timeout": {
      "types": "./src/utils/timeout.ts",
      "development": "./src/utils/timeout.ts",
      "default": "./dist/utils/timeout.js"
    },
    "./utils/price": {
      "types": "./src/utils/price.ts",
      "development": "./src/utils/price.ts",
      "default": "./dist/utils/price.js"
    },
    "./errors": {
      "types": "./src/errors/index.ts",
      "development": "./src/errors/index.ts",
      "default": "./dist/errors/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build --force && tsc-alias",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist node_modules",
    "lint": "biome lint ."
  },
  "devDependencies": {
    "@types/node": "^24.10.9",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.7.2",
    "vitest": "catalog:"
  }
}
