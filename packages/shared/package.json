{
  "name": "@workspace/shared",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "development": "./src/types/index.ts",
      "default": "./dist/types/index.js"
    },
    "./checkers": {
      "types": "./src/checkers/index.ts",
      "development": "./src/checkers/index.ts",
      "default": "./dist/checkers/index.js"
    },
    "./url-parser": {
      "types": "./src/url-parser.ts",
      "development": "./src/url-parser.ts",
      "default": "./dist/url-parser.js"
    },
    "./worker": {
      "types": "./src/worker/index.ts",
      "development": "./src/worker/index.ts",
      "default": "./dist/worker/index.js"
    }
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist node_modules",
    "lint": "biome lint ."
  },
  "dependencies": {
    "@workspace/db": "workspace:*",
    "axios": "^1.13.5",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.9",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "bullmq": "^5.0.0",
    "ioredis": "^5.0.0",
    "playwright": "^1.58.0"
  },
  "peerDependenciesMeta": {
    "playwright": {
      "optional": true
    },
    "bullmq": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    }
  }
}
