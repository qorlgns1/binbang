{
  "name": "@workspace/db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "default": "./dist/src/index.js"
    },
    "./client": {
      "types": "./src/client.ts",
      "development": "./src/client.ts",
      "default": "./dist/src/client.js"
    },
    "./enums": {
      "types": "./src/enums.ts",
      "development": "./src/enums.ts",
      "default": "./dist/src/enums.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:seed:base": "tsx prisma/seed-base.ts",
    "db:studio": "prisma studio",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf generated node_modules"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "pg": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.9",
    "@types/pg": "^8.11.11",
    "dotenv": "^17.2.3",
    "prisma": "^7.3.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
