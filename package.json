  "dependencies": {
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "next-auth": "^4.24.11",
    "@prisma/client": "^6.2.0",
    "prisma": "^6.2.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "puppeteer": "^24.0.0",
    "node-cron": "^3.0.3",
    "axios": "^1.7.9",
    "zod": "^3.24.1",
    "date-fns": "^4.1.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.6.0",
    "lucide-react": "^0.469.0"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/node-cron": "^3.0.11",
    "tailwindcss": "^3.4.17",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.0",
    "tsx": "^4.19.2"
  }
