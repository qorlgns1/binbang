{
  "name": "accommodation-monitor",
  "version": "2.11.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@workspace/web",
    "dev:worker": "turbo run dev --filter=@workspace/worker",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=@workspace/web",
    "build:worker": "turbo run build --filter=@workspace/worker",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "ci:check": "turbo run lint typecheck test build",
    "db:generate": "turbo run db:generate --filter=@workspace/db",
    "db:push": "pnpm --filter @workspace/db db:push",
    "db:migrate": "pnpm --filter @workspace/db db:migrate",
    "db:migrate:deploy": "pnpm --filter @workspace/db db:migrate:deploy",
    "db:seed": "pnpm --filter @workspace/db db:seed",
    "db:studio": "pnpm --filter @workspace/db db:studio",
    "clean": "turbo run clean && rm -rf node_modules",
    "local:docker": "docker compose -f docker/docker-compose.local.yml"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@tanstack/react-query": "^5.90.20",
    "axios": "^1.13.4",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.563.0",
    "next": "^15.5.12",
    "next-auth": "^4.24.11",
    "node-cron": "^3.0.3",
    "pg": "^8.18.0",
    "prisma": "^7.3.0",
    "puppeteer": "^24.36.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "radix-ui": "^1.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "2.15.4",
    "shadcn": "^3.8.1",
    "tailwind-merge": "^3.4.0",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.4.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "turbo": "^2.5.3",
    "@eslint/js": "^9.39.2",
    "@next/eslint-plugin-next": "^16.1.6",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/node": "^24.10.9",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.11",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.0.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.2.0",
    "jiti": "^2.6.1",
    "postcss": "^8.4.49",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "engines": {
    "node": ">=24.13.0 <25"
  }
}
