{
  "name": "@workspace/worker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "start": "node dist/main.js",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "test": "vitest run --passWithNoTests",
    "snapshot:public-availability": "tsx src/publicAvailabilitySnapshotOnce.ts",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@workspace/db": "workspace:*",
    "@workspace/shared": "workspace:*",
    "@workspace/worker-shared": "workspace:*",
    "bullmq": "^5.67.3",
    "dotenv": "^16.6.0",
    "ioredis": "^5.9.2",
    "playwright": "^1.58.2"
  },
  "devDependencies": {
    "@types/node": "^22.16.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "catalog:"
  }
}
